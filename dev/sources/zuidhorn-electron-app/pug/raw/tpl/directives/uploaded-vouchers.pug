div(ng-show="csv_content.show")
    .section.section-title Vouchers
    .section.section-panel
        | {{ csv_content.data.rows2.inputData((csv_content.data.rows | filter:{"0": 223796236})).flow() }}
        .table-responsive: table.table.table-stripped
            thead: tr
                th(ng-repeat="header in csv_content.data.headers") {{ header }}
                th
                    | ACTIVATED
                    a(href="#" ng-click="updateActivationList($event)").mdi.mdi-refresh.text-primary
            tbody: tr(ng-repeat="row in csv_content.data.rows2.flow()")
                td: strong.text-primary.mono {{ '#' + row[0] }}
                td {{ row[1] }}
                td.mono {{ row[2] }}
                td(ng-class="{'text-primary': activated[row[2]]}") {{ activated[row[2]] ? 'Yes' : 'No' }}
        div(pagination paginator="csv_content.data.rows2.paginator").pull-left
        .pagination: .pull-right
            a(href="#" ng-click="saveFromServer($event)").button.button-primary
                em.mdi.mdi-download
                | Download
            a(href="#" ng-click="deleteLocalData($event)").button.button-danger 
                em.mdi.mdi-delete
                | Delete